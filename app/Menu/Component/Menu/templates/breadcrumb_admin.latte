<div class="component-menu-tabs">
    <ul class="nav nav-tabs" n:inner-foreach="$breadcrumb as $node">
        <li n:class="$presenter->isLinkCurrent($node->link,array('id'=>$node->link_id))?active">
            <a href="{plink $node->link, id=>$node->link_id}">{$node->title}</a>
        </li>
    </ul>
</div>