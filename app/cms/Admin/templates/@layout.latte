{layout $baseLayout}
{#layout}
{control menu admin}
<div class="row">
    <div class="large-9 columns push-3">
        {control menu:breadcrumb}
        <div n:foreach="$flashes as $flash" data-alert class="alert-box {$flash->type}">
            {$flash->message}
            <a href="#" class="close">&times;</a>
        </div>
        {include #module}
    </div>
    <div class="large-3 columns pull-9">
        {block #sidebar}{/block}
    </div>
</div>