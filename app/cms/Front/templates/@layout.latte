{layout $baseLayout}
{#layout}
{control menu front}
<div class="row">
    <div class="small-12 columns">
        {control menu:breadcrumb}
        <div n:foreach="$flashes as $flash" data-alert class="alert-box {$flash->type}">
            {$flash->message}
            <a href="#" class="close">&times;</a>
        </div>
        {include #module}
    </div>
</div>