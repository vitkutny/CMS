{layout $baseLayout}
{#layout}
{control menu:navbar front}
<div class="container">
    {control menu:breadcrumb}
    <div n:foreach="$flashes as $flash" class="alert alert-{$flash->type}">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        {$flash->message}
    </div>
    {include #module}
</div>